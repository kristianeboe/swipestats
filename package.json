    "@splitbee/node": "^0.3.1",
    "@splitbee/web": "^0.3.0",
    "@types/debug": "^4.1.7",
    "chart.js": "^3.6.2",
    "chartjs-adapter-date-fns": "^2.0.0",
    "date-fns": "^2.27.0",
    "debug": "^4.3.3",
    "js-cookie": "^3.0.1",
    "JSONStream": "^1.3.5",
    "ky": "^0.28.7",
    "ky-universal": "^0.9.1",
    "mixpanel": "^0.14.0",
    "mixpanel-browser": "^2.42.0",
    "next": "^12.0.7",
    "parse-json": "^6.0.2",
